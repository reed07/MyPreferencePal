<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/mfp_orange"/>
            <padding android:left="10dp"/>
            <corners android:radius="2dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ffffff"/>
            <corners android:topRightRadius="2dp" android:bottomRightRadius="2dp"/>
            <padding android:top="20dp" android:bottom="20dp"/>
        </shape>
    </item>
</layer-list>
