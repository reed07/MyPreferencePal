<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:left="8dp">
        <shape android:shape="ring" android:innerRadiusRatio="3" android:useLevel="false" android:thickness="2.5dp">
            <solid android:color="@color/fuchsia"/>
            <size android:height="25dp" android:width="25dp"/>
        </shape>
    </item>
    <item android:left="8dp">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="@color/fuchsia"/>
            <stroke android:width="15dp" android:color="@android:color/transparent"/>
            <size android:height="25dp" android:width="25dp"/>
        </shape>
    </item>
</layer-list>
