<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape>
            <solid android:color="@color/black_20pct"/>
        </shape>
    </item>
    <item android:bottom="1dp">
        <shape>
            <solid android:color="@color/black_10pct"/>
        </shape>
    </item>
    <item android:left="1dp" android:right="1dp" android:bottom="1dp">
        <shape>
            <solid android:color="@color/transparent"/>
        </shape>
    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp">
        <selector>
            <item android:state_pressed="true">
                <shape>
                    <solid android:color="#c0c0c0"/>
                </shape>
            </item>
            <item>
                <shape>
                    <solid android:color="#f5f5f5"/>
                </shape>
            </item>
        </selector>
    </item>
</layer-list>
